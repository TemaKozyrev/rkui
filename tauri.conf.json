{
  "$schema": "https://schema.tauri.app/config-2.json",
  "productName": "rkui",
  "version": "0.1.0",
  "identifier": "com.example.rkui",
  "build": {
    "frontendDist": "web/dist",
    "beforeDevCommand": "npm run dev --prefix web",
    "beforeBuildCommand": "npm run build --prefix web",
    "devUrl": "http://127.0.0.1:5173"
  },
  "app": {
    "windows": [
      {
        "title": "rkui",
        "width": 1200,
        "height": 1000,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "icon": [
      "assets/icon.svg"
    ]
  }
}
