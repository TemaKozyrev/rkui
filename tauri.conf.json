{
  "$schema": "https://schema.tauri.app/config-2.json",
  "productName": "rkui",
  "version": "0.1.0",
  "identifier": "com.example.rkui",
  "build": {
    "frontendDist": "web/dist",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://127.0.0.1:5173"
  },
  "app": {
    "security": {
      "capabilities": ["default"]
    },
    "windows": [
      {
        "title": "rkui",
        "width": 1200,
        "height": 1000,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png"
    ]
  }
}
